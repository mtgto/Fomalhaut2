{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": [
          "last 1 chrome version",
          "last 1 edge version",
          "last 1 firefox version",
          "last 1 safari version",
          "last 1 ios version",
          "last 1 android version"
        ]
      }
    ],
    "@babel/typescript",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    "@emotion/babel-plugin",
    [
      "@babel/plugin-transform-react-jsx",
      {
        "runtime": "automatic"
      }
    ]
  ]
}
