  "license": "GPL-3.0-only",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vite",
    "build": "pnpm exec rimraf ../Fomalhaut2/assets.zip && vite build && zip -rj9 ../Fomalhaut2/assets.zip ./dist",
