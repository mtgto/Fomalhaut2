{
  "name": "web",
  "version": "1.0.0",
  "license": "GPL-3.0-only",
  "private": true,
  "scripts": {
    "serve": "parcel serve src/index.html",
    "build": "yarn run rimraf ./dist ../Fomalhaut2/assets.zip && parcel build ./src/index.html --public-url /assets/ && zip -rj9 ../Fomalhaut2/assets.zip ./dist",
    "lint": "yarn run lint:tsc && yarn run lint:eslint",
    "lint:eslint": "eslint $npm_config_eslint_args '*/**/*.{ts,tsx}'",
    "lint:tsc": "tsc --noEmit",
    "prepare": "cd .. && husky install web/.husky"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0",
    "history": "^5.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rocon": "^1.2.0"
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@jamesacarr/eslint-formatter-github-actions": "^0.1.0",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7",
    "lint-staged": "^11.1.2",
    "parcel": "^2.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "browserslist": [
    "last 1 chrome version",
    "last 1 edge version",
    "last 1 firefox version",
    "last 1 safari version",
    "last 1 ios version",
    "last 1 android version"
  ],
  "packageManager": "yarn@3.1.1"
}
