{
  "name": "web",
  "version": "1.0.0",
  "license": "GPL-3.0-only",
  "private": true,
  "scripts": {
    "serve": "parcel serve src/index.html",
    "build": "yarn run rimraf ./dist ../Fomalhaut2/assets.zip && parcel build ./src/index.html --public-url /assets/ && zip -rj9 ../Fomalhaut2/assets.zip ./dist",
    "lint": "yarn run lint:tsc && yarn run lint:eslint",
    "lint:eslint": "eslint ${ESLINT_ARGS:-''} '*/**/*.{ts,tsx}'",
    "lint:tsc": "tsc --noEmit",
    "prepare": "cd .. && husky install web/.husky"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.0",
    "history": "^5.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rocon": "^1.2.5"
  },
  "devDependencies": {
    "@jamesacarr/eslint-formatter-github-actions": "^0.1.0",
    "@types/node": "^16.11.36",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "parcel": "^2.5.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4"
  },
  "browserslist": [
    "last 1 chrome version",
    "last 1 edge version",
    "last 1 firefox version",
    "last 1 safari version",
    "last 1 ios version",
    "last 1 android version"
  ],
  "packageManager": "yarn@3.2.0"
}
