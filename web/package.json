{
  "name": "web",
  "version": "1.0.0",
  "license": "GPL-3.0-only",
  "private": true,
  "scripts": {
    "serve": "parcel serve src/index.html",
    "build": "yarn run rimraf ./dist ../Fomalhaut2/assets.zip && parcel build ./src/index.html --public-url /assets/ && zip -rj9 ../Fomalhaut2/assets.zip ./dist",
    "lint": "yarn run lint:tsc && yarn run lint:eslint",
    "lint:eslint": "eslint ${ESLINT_ARGS:-''} '*/**/*.{ts,tsx}'",
    "lint:tsc": "tsc --noEmit",
    "prepare": "cd .. && husky install web/.husky"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.4",
    "history": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rocon": "^1.2.8"
  },
  "devDependencies": {
    "@jamesacarr/eslint-formatter-github-actions": "^0.1.0",
    "@types/node": "^16.11.58",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.7",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2"
  },
  "browserslist": [
    "last 1 chrome version",
    "last 1 edge version",
    "last 1 firefox version",
    "last 1 safari version",
    "last 1 ios version",
    "last 1 android version"
  ],
  "packageManager": "yarn@3.2.0"
}
